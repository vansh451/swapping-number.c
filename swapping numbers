//C Program to perform the swapping of 2 numbers without using a temporary variable.

#include<stdio.h>
#include<conio.h>

int main(){
    int a=15;
    int b=10;
    printf("Before swapping a=%d b=%d\n",a,b);
    a=a+b;//a=15+10=25
    b=a-b;//a=25-10=15
    a=a-b;//a=25-15=10
    printf("After swapping a=%d b=%d",a,b);
    return 0;  
}
